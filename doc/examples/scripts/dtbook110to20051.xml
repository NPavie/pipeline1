<taskScript version="1.0">
	<name>DTBook 2005-1 to 2005-2</name>
	<description>Converts a DTBook version 2005-1 document to version 2005-2</description>
	
	<task name="int_daisy_dtbookMigrator" interactive="false">

		<parameter>
			<name>input</name>
			<value>C:/data/workspace/dmfc/doc/examples/input/hauy-2005-1.xml</value>
		</parameter>
					
		<parameter>
			<name>output</name>
			<value>C:/data/temp/dmfc-out</value>
		</parameter>		
		
	 	<parameter>
			<name>outputVersion</name>
			<value>2005-1</value>
		</parameter>
				
	</task>

</taskScript>
