<taskScript version="1.0">
	<name>DTBook2Text</name>
	<description>This script converts a DTBook document to text</description>
	
	<task name="uk_rnib_dtbook2text" interactive="false">

		<parameter>
			<name>xml</name>
			<value>d:/books/tb50-2.xml</value>
		</parameter>
					
		<parameter id="id1">
			<name>out</name>
			<value>d:/books/test.txt</value>
		</parameter>		
				
	</task>

</taskScript>
