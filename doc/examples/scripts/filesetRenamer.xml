<taskScript version="1.0">
	<name>Fileset Renamer</name>
	<description>Rename select members of a fileset using customizable token-based name patterns</description>
	
	<task name="int_daisy_filesetRenamer" interactive="false">

		<parameter>
			<name>input</name>
			<value>C:/data/temp/dmfc-in/202fileset/ncc.html</value>
		</parameter>

		<parameter>
			<name>output</name>
			<value>C:/data/temp/dmfc-out/</value>
		</parameter>

		<parameter>
			<name>renamingPattern</name>
			<value>uid+seq</value>
			<!-- <value>echo</value> -->
			<!-- <value>uid+rnd(2)</value> -->
		</parameter>	
		
		<parameter>
			<name>exclude</name>
			<value>None</value>
		</parameter>
											
	</task>

</taskScript>
