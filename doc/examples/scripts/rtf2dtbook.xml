<taskScript version="1.0">
	<name>RTF2DTBook</name>
	<description>This script converts an RTF document to DTBook</description>
	
	<task name="ca_cnib_rtf2dtbook" interactive="false">

		<parameter>
			<name>rtf</name>
			<value>d:/input.rtf</value>
		</parameter>
					
		<parameter id="id1">
			<name>dtbook</name>
			<value>d:/output-rtf.dtbook</value>
		</parameter>		
				
	</task>

</taskScript>
