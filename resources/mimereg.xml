<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE registry [
  <!ELEMENT registry (type+)>
  <!ELEMENT type EMPTY>
  <!ATTLIST type
  	 id ID #REQUIRED
  	 name CDATA #REQUIRED
  	 parentType IDREFS #IMPLIED
  	 definition CDATA #IMPLIED
  >
]>

<registry>

	<!-- Other -->
	<type id="string" name="string"/>
	<type id="enum" name="enum" parentType="string"/>
	<type id="folder" name="application/x-filesystemDirectory"/>
	<type id="rtf" name="application/rtf"/>

	<!-- Images -->
	<type id="jpg" name="image/jpeg"/>
	<type id="gif" name="image/gif"/>
	<type id="png" name="image/png"/>
	
	<!-- Audio -->
	<type id="mp3" name="audio/x-mpeg"/>
	<type id="wav" name="audio/x-wav"/>

	<!-- TEXT -->
	<type id="plain" name="text/plain"/>
	<type id="css" name="text/css"/>
	<type id="latex" name="application/x-latex"/>

	<!-- HTML -->
	<type id="html" name="text/html"/>

	<!-- XML -->
	<type id="xml" name="application/xml"/>
	<type id="xhtml" name="application/xhtml+xml" parentType="xml html"/>
	<type id="xslt" name="application/xslt+xml" parentType="xml" />		

	<!-- DAISY 2.02 -->
	<type id="daisy2.02" name="application/x-dtbfileset-2.02+xml" definition="The navigation control center member of the DAISY 2.02 fileset."/>	
	<type id="ncc2.02" name="application/x-ncc-2.02+xml" parentType="xhtml"/>
	<type id="smil2.02" name="application/x-smil-2.02+xml" parentType="xml"/>
	<type id="mastersmil2.02" name="application/x-mastersmil-2.02+xml" parentType="xml"/>	
	<type id="ldabadpcm2.02" name="audio/x-ldabadpcm"/>

	<!-- DAISY z39.86 -->
	<type id="dtbook" name="application/x-dtbook+xml" parentType="xml"/>
	<type id="ncx" name="application/x-dtbncx+xml" parentType="xml"/>
	<type id="smil" name="application/smil" parentType="xml"/> 
	<type id="dtbresource" name="application/x-dtbresource+xml" parentType="xml"/>	
	
	<!-- Open Document Format -->
	<type id="odt" name="application/vnd.oasis.opendocument.text"/>
	
	<!-- binary -->
	<type id="pdf" name="application/pdf"/>

</registry>
