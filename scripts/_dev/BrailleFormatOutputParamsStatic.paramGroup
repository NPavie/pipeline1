<?xml version="1.0" encoding="utf-8"?>
<paramGroup>

	<parameter name="table" value="en_US" required="true">
		<nicename>Target format</nicename>
		<description>Target format</description>
		<datatype>
			<enum>
			  <item nicename="North American ASCII Braille" value="en_US" />
			  <item nicename="Unicode braille" value="UNICODE_BRAILLE" />
			  <item nicename="Danish" value="da_DK" />
			  <item nicename="German" value="de_DE" />
			  <item nicename="Spanish" value="es_ES" />
			  <item nicename="Italian (Firenze)" value="it_IT_FIRENZE" />
			  <item nicename="Swedish - CX" value="sv_SE_CX" />
			</enum>
		</datatype>
	</parameter>
	
	<parameter name="pad" value="both" required="false">
		<nicename>Form feed policy, insert newline</nicename>
		<description>Define where to insert newline around form feed</description>
		<datatype>
			<enum>
			  <item nicename="Before" value="before" />
			  <item nicename="After" value="after" />
			  <item nicename="Before and after" value="both" />
			  <item nicename="Neither before nor after" value="none" />
			</enum>
		</datatype>
	</parameter>
		
</paramGroup>