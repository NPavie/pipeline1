			</tag>
			<!--  -->
			<tag name="Body Text Indent">
				<wrap value="blockquote" addId="false" merge="true">
					<using value="p"/>
				</wrap>
			</tag>
			<tag name="List" listOverride="false">
				<wrap value="list" addId="false" merge="true">
					<using value="li"/>
					<attribute name="type" value="ol"/>
				</wrap>
			</tag>
		</paragraphs>
