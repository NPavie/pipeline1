<?xml version="1.0" encoding="UTF-8"?>
<mappings version="2006-1" xmlns="http://www.tpb.se/ns/2006/wml2dtbook" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.tpb.se/ns/2006/wml2dtbook tagset.xsd">
	<standardWord version="4">
		<paragraphs>
			<tag name="heading 1" listOverride="true"><map value="h1"/></tag>
			<tag name="heading 2" listOverride="true"><map value="h2"/></tag>
			<tag name="heading 3" listOverride="true"><map value="h3"/></tag>
			<tag name="heading 4" listOverride="true"><map value="h4"/></tag>
			<tag name="heading 5" listOverride="true"><map value="h5"/></tag>
			<tag name="heading 6" listOverride="true"><map value="h6"/></tag>
			<tag name="footnote text">
				<wrap value="note" addId="true" merge="false">
					<using value="p"/>
				</wrap>
			</tag>
			<!-- Indraget Stycke -->
			<tag name="Block Text">
				<wrap value="blockquote" addId="false" merge="true">
					<using value="p"/>
				</wrap>
			</tag>
			<tag name="Body Text Indent">
				<wrap value="blockquote" addId="false" merge="false">
					<using value="p"/>
				</wrap>
			</tag>
			<!-- Kommentarer -->
			<tag name="annotation text"><comment/></tag>
			<tag name="caption">
				<wrap value="imggroup" addId="false" merge="true">
					<using value="caption"/>
				</wrap>
			</tag>
		</paragraphs>
		<character>
			<tag name="Strong"><map value="strong"/></tag>
			<tag name="Emphasis"><map value="em"/></tag>
			<tag name="page number"><pagenum/></tag>
			<tag name="footnote reference"><noteref/></tag>
		</character>
	</standardWord>
</mappings>
